"time_integ":"1",      # 1 = explicit, 2 = lusgs
"nt":"1.0e4",          # 時間ステップ数
"dt":"1.0e-6",       # 時間刻み幅[s]
"every_outnum":"5.0e2",      # 出力毎タイムステップ
