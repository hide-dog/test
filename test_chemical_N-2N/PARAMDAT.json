# -- parameter --
{
# -- file name --
"out_file_front":"test", # 出力ファイル前半分
"out_ext":".dat",         # 拡張子

# -- restart --
"Restart":"test",         # 前半のファイル名
"restartnum":"010",     # 後半のファイル名（result/[Restart+restartnum+in_ext]で検索）
"in_ext":".dat",

# -- output --
"init_small":"1.0e-50",       # 初期発散抑え用(陽解法ならいらない)
"norm_ok":"1.0e-4",          # 収束条件(陽解法ならいらない)

# -- time step --
"time_integ":"1",      # 1 = explicit, 2 = lusgs
"nt":"1.0e2",          # 時間ステップ数
"dt":"1.0e-6",       # 時間刻み幅[s]
"every_outnum":"1.0e1",      # 出力毎タイムステップ
"inner_step":"10",       # 疑似時間ステップ数
"dtau":"1.0e-2",       # 疑似時間刻み幅[s]

# -- 初期値 --
"init_rho":"1.0e0",     # 初期密度[kg/m^3]
"init_u":"500.0",       # 初期x方向速度
"init_v":"0.0",       # 初期y方向速度
"init_p":"370.6",      # 初期圧力[Pa] don't use
"init_T":"300.0",      # 初期温度[K]
"init_pt":"1",         # 1 = 温度基準
"init_N2":"1.0",    # 質量分率
"init_N":"0.0",       # 質量分率

"specific_heat_ratio":"1.4", # 比熱比
"Rd":"287.0",                   # 空気の気体定数[J/kgK]

# -- boundary condition --
# left_bd1 = 境界条件
# 00 ; 流入条件 (圧力参照)
# 01 ;         (密度固定，温度参照)
# 02 ; 0~1/4 and 3/4~1 流入条件 (圧力参照)
# 03 ;                         (密度固定，温度参照)
# 20 ; 流出条件
# 30 ; 断熱壁(rho_wall = rho, u=v=0, p_wall = p)
# 31 ; 等温壁(rho_wall = p/ΣcRT, u=v=0, p_wall = p)
# 40 ; 周期境界条件
# x- --------------------------------
"bd1_con":"20",
"bd1_rho":"1.0",
"bd1_u":"0.0",
"bd1_v":"0.0",
"bd1_p":"1.0",
"bd1_N2":"1.0",
"bd1_N":"0.0",
"bd1_T":"300.0",
# x+ -------------------------------
"bd2_con":"20",
"bd2_rho":"1.0",
"bd2_u":"0.0",
"bd2_v":"0.0",
"bd2_p":"1.0",
"bd2_N2":"1.0",
"bd2_N":"0.0",
"bd2_T":"300.0",
# y- --------------------------------
"bd3_con":"30",
"bd3_rho":"1.0",
"bd3_u":"0.0",
"bd3_v":"0.0",
"bd3_p":"0.0",
"bd3_N2":"1.0",
"bd3_N":"0.0",
"bd3_T":"300.0",
# y+ --------------------------------
"bd4_con":"00",
"bd4_rho":"1.2e0",
"bd4_u":"3000.0",
"bd4_v":"0.0",
"bd4_p":"1.0e5",   #p=rhoRTより
"bd4_N2":"1.0",
"bd4_N":"0.0",
"bd4_T":"300.0",
#--------------------------------
"blank":"blank"
}
